{
  "name": "soroban-simulators",
  "version": "1.3.0",
  "description": "Soroban simulators for training",
  "main": "lib/index.js",
  "scripts": {
    "start": "npm run compile && node lib/index.js",
    "build-css": "node-sass --include-path scss src/sass/style.sass lib/css/style.css",
    "test": "npm run compile",
    "release": "standard-version",
    "compile": "npm run build-css && babel --presets es2015 -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/web-west/soroban-simulators.git"
  },
  "keywords": [
    "soroban tests",
    "soroban training",
    "soroban simulators"
  ],
  "author": "web-west",
  "license": "MIT",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/web-west/soroban-simulators/issues"
  },
  "homepage": "https://web-west.github.io/demo-soroban-simulators/",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "node-sass": "^4.12.0",
    "standard-version": "^6.0.1"
  }
}
